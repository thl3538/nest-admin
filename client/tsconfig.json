{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts"],
    "exclude": ["src/**/__tests__/*"],
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "node",
      "composite": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "cm/*": ["./src/common/*"],
        "_hooks": ["./src/hooks"],
      },
      "types": ["vite/client", "element-plus/global"]
    }
  }
  